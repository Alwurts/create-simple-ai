{
  "name": "{{projectName}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write src",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down -v",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@hono/zod-validator": "{{version versions "@hono/zod-validator"}}",
    "@hookform/resolvers": "{{version versions "@hookform/resolvers"}}",
    "@radix-ui/react-checkbox": "{{version versions "@radix-ui/react-checkbox"}}",
    "@radix-ui/react-dialog": "{{version versions "@radix-ui/react-dialog"}}",
    "@radix-ui/react-label": "{{version versions "@radix-ui/react-label"}}",
    "@radix-ui/react-slot": "{{version versions "@radix-ui/react-slot"}}",
    "@t3-oss/env-nextjs": "{{version versions "@t3-oss/env-nextjs"}}",
    "@tanstack/react-query": "{{version versions "@tanstack/react-query"}}",
    "better-auth": "{{version versions "better-auth"}}",
    "class-variance-authority": "{{version versions "class-variance-authority"}}",
    "clsx": "{{version versions "clsx"}}",
    "drizzle-orm": "{{version versions "drizzle-orm"}}",
    "hono": "{{version versions "hono"}}",
    "lucide-react": "{{version versions "lucide-react"}}",
    "next": "{{version versions "next"}}",
    "next-themes": "{{version versions "next-themes"}}",
    "react": "{{version versions "react"}}",
    "react-dom": "{{version versions "react-dom"}}",
    "react-hook-form": "{{version versions "react-hook-form"}}",
    "sonner": "{{version versions "sonner"}}",
    "tailwind-merge": "{{version versions "tailwind-merge"}}",
    "zod": "{{version versions "zod"}}"{{#if (eq database "postgres")}},
    "pg": "{{version versions "pg"}}"{{/if}}{{#if (eq database "mysql")}},
    "mysql2": "{{version versions "mysql2"}}"{{/if}}{{#if (eq database "sqlite")}},
    "@libsql/client": "{{version versions "@libsql/client"}}"{{/if}}
  },
  "devDependencies": {
    "@biomejs/biome": "{{version versions "@biomejs/biome"}}",
    "@tailwindcss/postcss": "{{version versions "@tailwindcss/postcss"}}",
    "@types/node": "{{version versions "@types/node"}}",
    "@types/react": "{{version versions "@types/react"}}",
    "@types/react-dom": "{{version versions "@types/react-dom"}}",
    "drizzle-kit": "{{version versions "drizzle-kit"}}",
    "tailwindcss": "{{version versions "tailwindcss"}}",
    "tw-animate-css": "{{version versions "tw-animate-css"}}",
    "typescript": "{{version versions "typescript"}}"{{#if (eq database "postgres")}},
    "@types/pg": "{{version versions "@types/pg"}}"{{/if}}
  }
}
